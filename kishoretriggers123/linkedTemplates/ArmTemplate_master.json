{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kishoretriggers123"
		},
		"ls_store_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_store'"
		},
		"trigger4_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8248eef1-55df-4aab-8cc4-d3cfe2616a62/resourceGroups/DevRG/providers/Microsoft.Storage/storageAccounts/triggersstorage123"
		},
		"trigger5_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8248eef1-55df-4aab-8cc4-d3cfe2616a62/resourceGroups/DevRG/providers/Microsoft.Storage/storageAccounts/triggersstorage123"
		},
		"trigger6_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8248eef1-55df-4aab-8cc4-d3cfe2616a62/resourceGroups/DevRG/providers/Microsoft.Storage/storageAccounts/triggersstorage123"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kishoretriggers123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_store_connectionString": {
						"value": "[parameters('ls_store_connectionString')]"
					},
					"trigger4_properties_typeProperties_scope": {
						"value": "[parameters('trigger4_properties_typeProperties_scope')]"
					},
					"trigger5_properties_typeProperties_scope": {
						"value": "[parameters('trigger5_properties_typeProperties_scope')]"
					},
					"trigger6_properties_typeProperties_scope": {
						"value": "[parameters('trigger6_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}